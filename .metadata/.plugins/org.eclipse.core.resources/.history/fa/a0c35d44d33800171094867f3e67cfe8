//Problem PRETNUM form Codechef
import java.io.*;
import java.util.*;
public class PRETNUM_Divisors {

	static InputReader in=new InputReader(System.in);
	static PrintWriter out=new PrintWriter(System.out);
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int t;
        long m,n;
        t=in.readInt();
    	ArrayList<Long> ans=sieve(1000000L);
        while(t-->0)
        {
        	m=in.readLong();
        	n=in.readLong();
        	long mark[]=new long[(int)(n-m+1)];
        	for(long i=m;i<=n;i++)
        		mark[(int)(i-m)]=i;
        	long div[]=new long[(int)(n-m+1)];
        	set(div,1L);
    		for(long i:ans)
    		{
    			for(long j=(long)Math.ceil((double)m/i)*i;j<=n;j++)
    	    	{
    	    		long tp=mark[(int)(j-m)];
    				int ct=0;
    				while(tp%i==0)
    				{
    					ct++;
    					tp/=i;
    				}
    				div[(int)(j-m)]*=ct+1;
    				mark[(int)(j-m)]=tp;
    			}
    		}
    		int count=0;
        	ArrayList<Long> list=sieve(5000L);
    		for(long i:list)
    			for(long j=m;j<=n;j++)
    			{
    				if(mark[(int)(j-m)]!=1)
    					div[(int)(j-m)]*=2;
    				if(div[(int)(j-m)]==i)
    					count++;
    			}
        	out.println(count);
        }
        out.flush();
        out.close();
	}
	
    public static ArrayList<Long> sieve(long n)
    {
    	int i,j;
    	ArrayList<Long> list=new ArrayList<Long>();
    	int n_n=(int)Math.sqrt(n);
    	boolean tp[]=new boolean[10000001];
    	tp[0]=tp[1]=true;
    	for(i=2;i<=n_n;i++)
    	{
    		if(!tp[i])
    		for(j=2*i;j<=n;j+=i)
    		{
    			tp[j]=true;
    		}
    	}
    	for(j=2;j<=n;j++)
    	  if(!tp[j])
    		list.add((long)j);
    	return list;
    }
    public static void set(long a[],long val)
	{
		for(int i=0;i<a.length;i++)
			a[i]=val;
	}
}
